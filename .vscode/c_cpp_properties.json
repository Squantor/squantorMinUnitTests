{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/squantorMinUnit/inc",
                "${workspaceFolder}/minunit_tests_PC/inc"
            ],
            "defines": [
                "DEBUG",
                "MINUNIT_REPORT_DISABLE",
